<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="us.duia.leejo0531.dao.UserMapper">

	<insert id="insertUserInfo" parameterType="User">
		insert into userInfo values(userInfo_seq.nextval, #{id}, #{pw}, #{userName}, #{age}, #{birth}, #{phone}, #{email})
	</insert>

	<insert id="insertUserField" parameterType="Field">
		insert into field values(#{fieldNum}, #{userNum}, #{majorNum}, #{minorNum});
	</insert>
</mapper>
